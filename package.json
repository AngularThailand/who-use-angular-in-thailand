    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "9.4.2",
    "ng-packagr": "5.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "protractor": "5.4.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsickle": "0.37.0",
